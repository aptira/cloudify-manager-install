[
 {ssl, [{versions, ['tlsv1.2', 'tlsv1.1']}]},
 {rabbit, [
           {loopback_users, []},
           {ssl_listeners, [5671]},
           {ssl_options, [{cacertfile,"{{ constants.ca_cert_path }}"},
                          {certfile,  "{{ constants.internal_cert_path }}"},
                          {keyfile,   "{{ constants.internal_key_path }}"},
                          {versions, ['tlsv1.2', 'tlsv1.1']},
                          {dhfile, "{{ constants.dhparams_path }}"}
                         ]}
 ]},
 {rabbitmq_management, [{load_definitions, "/etc/rabbitmq/definitions.json"}]}
].
